print welcome
if(1 < 10)
    print(hi)
endif
print how are you?
---------

print welcome
cmp 1, 10            1 < 10 ?
jl PL0               1 < 10 ? -> counter
jmp PL1              else counter + 1
PL0:                 counter
    print hi         continue normally
    jmp PL1          endif, increment counter
PL1:                 endif: counter
    how are you?    continue normally
